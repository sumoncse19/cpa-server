{
  "name": "cpa-backend",
  "version": "1.0.0",
  "description": "Node.js backend with Express, TypeScript, and Sequelize",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "migrate": "NODE_ENV=development ./node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "NODE_ENV=development ./node_modules/.bin/sequelize-cli db:migrate:undo",
    "seed": "NODE_ENV=development ./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "NODE_ENV=development ./node_modules/.bin/sequelize-cli db:seed:undo:all",
    "migrate:prod": "NODE_ENV=production ./node_modules/.bin/sequelize-cli db:migrate",
    "seed:prod": "NODE_ENV=production ./node_modules/.bin/sequelize-cli db:seed:all",
    "postbuild": "npm run migrate:prod && npm run seed:prod"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.44",
    "@types/pg": "^8.15.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.35.1",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumoncse19/cpa-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/sumoncse19/cpa-server/issues"
  },
  "homepage": "https://github.com/sumoncse19/cpa-server#readme"
}
